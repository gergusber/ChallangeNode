  "bugs": {
    "url": "https://github.com/gergusber/ChallangeNode/issues"
  },
  "homepage": "https://github.com/gergusber/ChallangeNode#readme",
  "devDependencies": {
    "@types/sequelize": "^4.28.10",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "mocha": "^9.0.3",
    "nodemon": "^2.0.12",
    "sequelize-cli": "^6.2.0",
    "sequelize-mock": "^0.10.2",
    "sinon": "^11.1.2"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.12.1",
    "mysql2": "^2.3.0",
    "sequelize": "^6.6.5",
    "supertest": "^6.1.4"
  }
